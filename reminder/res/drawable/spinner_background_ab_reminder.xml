<?xml version="1.0" encoding="utf-8"?>

<!-- Spinner Selector -->
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:drawable="@drawable/spinner_ab_disabled_reminder" android:state_enabled="false"/>
    <item android:drawable="@drawable/spinner_ab_pressed_reminder" android:state_pressed="true"/>
    <item android:drawable="@drawable/spinner_ab_focused_reminder" android:state_focused="true" android:state_pressed="false"/>
    <item android:drawable="@drawable/spinner_ab_default_reminder"/>

</selector>